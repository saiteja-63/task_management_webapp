{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off"
  },
  "ignorePatterns": ["*"],
  "overrides": [
    {
      "files": ["*.test.js", "*.spec.js"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ]
}